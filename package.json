{
	"name": "ansaars-icon-theme",
	"displayName": "ansaars-icon-theme",
	"description": "My custom icon theme, derived from tal7aouy/vscode-icons",
	"version": "0.0.1",
	"publisher": "ansaardollie",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Themes"
	],
	"activationEvents": [
		"onIdentity:identity"
	],
	"main": "./out/extension.js",
	"contributes": {
		"iconThemes": [
			{
				"id": "ansaar-icon-theme",
				"label": "Ansaar's Icon Theme",
				"path": "./icons.json"
			}
		]
	},
	"icon": "icon.png",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"build": "tsc && node ./out/extension.js",
		"package": "vsce package",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^8.0.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "^16.18.11",
		"@types/vscode": "^1.74.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@vscode/test-electron": "^2.2.0",
		"eslint": "^8.28.0",
		"glob": "^8.0.3",
		"mocha": "^10.1.0",
		"typescript": "^4.9.3"
	}
}
